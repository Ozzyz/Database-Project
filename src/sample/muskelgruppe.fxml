<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ComboBox?>

<GridPane fx:controller="sample.muskelgruppeController" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Label GridPane.columnIndex="0" GridPane.rowIndex="0" text="Muskelgruppe: "/>
    <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" fx:id="muskelgruppe"/>

    <Label GridPane.columnIndex="0" GridPane.rowIndex="1" text="Undergruppe av: "/>
    <ComboBox GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="undergruppeComboBox" promptText="Velg"/>

    <Button GridPane.columnIndex="0" GridPane.rowIndex="2" text="Avbryt" onAction="#backButtonClickd"/>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="2" text="Registrer" onAction="#registerButtonClicked"/>

</GridPane>